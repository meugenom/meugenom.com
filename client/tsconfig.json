{
	"compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./target/classes/static/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2018",
    "lib": ["es2018", "dom"]
  },
  "include": [
    "./src/js/**/*",
    "./src/types/**/*"
  ]
}
